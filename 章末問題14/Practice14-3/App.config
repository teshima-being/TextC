<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="myAppSettings"
             type="Practice14_3.MyAppSettings, Practice14-3"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <myAppSettings>
    <traceOption enabled="true"
                 filePath="C:\MyApp\Trace.log"
                 bufferSize="10240" />
    <calendarOption stringFormat="yyyy年MM月dd日(ddd)"
                    minimum="1900/1/1"
                    maximum="2100/12/31"
                    mondayIsFirstDay="True" />
  </myAppSettings>
</configuration>